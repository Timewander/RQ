RewriteCond %{QUERY_STRING} ^(.+)$
RewriteRule ^([^/.]+)/([^/]+)$ index.php?P_C=$1&P_A=$2&%1
RewriteRule ^([^/.]+)/([^/]+)$ index.php?P_C=$1&P_A=$2

RewriteCond %{QUERY_STRING} ^(.+)$
